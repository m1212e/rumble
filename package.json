{
  "name": "@m1212e/rumble",
  "module": "./out/index.js",
  "version": "0.12.0",
  "type": "module",
  "repository": "https://github.com/m1212e/rumble",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "format": "bun run lint",
    "lint": "bunx biome check --write .",
    "dev:docker": "docker compose -f ./dev.docker-compose.yml up",
    "dev:example": "bun ./example/src/main.ts",
    "dev:example:seed": "bun example/src/db/seed.ts",
    "build": "tsdown",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@graphql-tools/executor-http": "^3.0.4",
    "@types/bun": "latest",
    "@types/pg": "^8.15.5",
    "@types/pluralize": "^0.0.33",
    "drizzle-kit": "^1.0.0-beta.1-ac4ce44",
    "drizzle-orm": "^1.0.0-beta.1-ac4ce44",
    "drizzle-seed": "^0.3.1-ac4ce44",
    "lefthook": "^1.13.6",
    "pg": "^8.16.3",
    "tsdown": "^0.15.6"
  },
  "peerDependencies": {
    "typescript": "^5",
    "drizzle-orm": "^1"
  },
  "dependencies": {
    "@escape.tech/graphql-armor": "^3.1.7",
    "@graphql-yoga/plugin-disable-introspection": "^2.17.0",
    "@pothos/core": "^4.10.0",
    "@pothos/plugin-drizzle": "^0.13.0",
    "@pothos/plugin-smart-subscriptions": "^4.1.4",
    "@urql/core": "^6.0.1",
    "@urql/exchange-graphcache": "^8.1.0",
    "es-toolkit": "^1.40.0",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "graphql-yoga": "^5.16.0",
    "pluralize": "^8.0.0",
    "sofa-api": "^0.18.8",
    "wonka": "^6.3.5"
  },
  "main": "./out/index.cjs",
  "types": "./out/index.d.cts",
  "exports": {
    ".": {
      "import": "./out/index.js",
      "require": "./out/index.cjs"
    },
    "./package.json": "./package.json"
  }
}
